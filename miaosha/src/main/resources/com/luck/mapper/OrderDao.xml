<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.luck.dao.OrderDao">
    <!--创建订单-->
    <insert id="save" parameterType="com.luck.entity.Order" useGeneratedKeys="true" keyProperty="id">
        insert into stock_order values(#{id},#{sid},#{name},#{creationDate},#{lastUpdateDate})
    </insert>
</mapper>
